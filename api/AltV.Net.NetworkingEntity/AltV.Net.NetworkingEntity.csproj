<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp2.2</TargetFramework>
    </PropertyGroup>

    <ItemGroup>
        <Protobuf Include="Client\entity.proto">
          <GrpcServices>Both</GrpcServices>
          <Access>Public</Access>
          <ProtoCompile>True</ProtoCompile>
          <ProtoRoot></ProtoRoot>
          <CompileOutputs>True</CompileOutputs>
          <OutputDir>obj\Debug\netcoreapp3.0\</OutputDir>
          <Link>Client\entity.proto</Link>
        </Protobuf>
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\AltV.Net\AltV.Net.csproj" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="VIEApps.Components.WebSockets" Version="10.2.1906.1" />
      <PackageReference Include="Google.Protobuf" Version="3.7.0" />
      <PackageReference Include="Grpc" Version="1.20.1" />
      <PackageReference Include="Grpc.Tools" Version="1.20.1" PrivateAssets="All" />
    </ItemGroup>

    <ItemGroup>
      <Compile Remove="Client\deps\**" />
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Remove="Client\deps\**" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="Client\deps\**" />
    </ItemGroup>

    <ItemGroup>
      <Compile Remove="Client\index.html" />
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Remove="Client\index.html" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="Client\index.html" />
    </ItemGroup>

    <ItemGroup>
      <Compile Remove="Client\networking-entity.js" />
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Remove="Client\networking-entity.js" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="Client\networking-entity.js" />
    </ItemGroup>

</Project>
